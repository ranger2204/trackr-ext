{
    "name": "TrackEmAll",
    "version": "1.0",
    "description": "TrackEmAll Chrome Extension",
    "manifest_version": 3,
    "action": {
        "default_popup": "index.html"
    },
    "permissions": [
        "tabs",
        "activeTab",
        "webNavigation",
        "scripting"
    ],
    "background": {
        "service_worker": "background.js"
    }
}